    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bestzip": "^1.1.4",
    "bravey": "^0.1.4",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "compromise": "^10.7.1",
    "css-loader": "^0.28.5",
    "cucumber": "^3.0.2",
