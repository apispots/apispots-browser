    "flatpickr": "^3.0.7",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.5.0",
    "helper-markdown": "^1.0.0",
    "html-loader": "^0.5.1",
    "joi": "^11.0.1",
    "js-yaml": "^3.10.0",
    "json2csv": "^3.11.2",
    "jsonexport": "^2.0.11",
    "lodash": "^4.17.4",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.18.1",
    "nconf": "^0.8.4",
    "postal": "^2.0.5",
    "pouchdb-browser": "^6.3.4",
